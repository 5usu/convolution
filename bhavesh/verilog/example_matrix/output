T_0.2 ;
    %load/vec4 v0x5c259a7543a0_0;
    %cmpi/s 2, 0, 32;
    %flag_or 5, 4;
    %jmp/0xz T_0.3, 5;
    %pushi/vec4 0, 0, 32;
    %store/vec4 v0x5c259a754480_0, 0, 32;
T_0.4 ;
    %load/vec4 v0x5c259a754480_0;
    %cmpi/s 2, 0, 32;
    %flag_or 5, 4;
    %jmp/0xz T_0.5, 5;
    %load/vec4 v0x5c259a73f8c0_0;
    %load/vec4 v0x5c259a7543a0_0;
    %muli 3, 0, 32;
    %load/vec4 v0x5c259a754480_0;
    %add;
    %muli 8, 0, 32;
    %part/s 8;
    %vpi_call 3 17 "$write", " %d", S<0,vec4,u8> {1 0 0};
    %load/vec4 v0x5c259a754480_0;
    %addi 1, 0, 32;
    %store/vec4 v0x5c259a754480_0, 0, 32;
    %jmp T_0.4;
T_0.5 ;
    %vpi_call 3 19 "$write", "\012" {0 0 0};
    %load/vec4 v0x5c259a7543a0_0;
    %addi 1, 0, 32;
    %store/vec4 v0x5c259a7543a0_0, 0, 32;
    %jmp T_0.2;
T_0.3 ;
T_0.0 ;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x5c259a754300_0, 0, 1;
    %jmp T_0;
    .thread T_0;
    .scope S_0x5c259a72bcd0;
T_1 ;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x5c259a7546d0_0, 0, 1;
T_1.0 ;
    %delay 5, 0;
    %load/vec4 v0x5c259a7546d0_0;
    %inv;
    %store/vec4 v0x5c259a7546d0_0, 0, 1;
    %jmp T_1.0;
    %end;
    .thread T_1;
    .scope S_0x5c259a72bcd0;
T_2 ;
    %vpi_call 2 18 "$dumpfile", "matrix_1.vcd" {0 0 0};
    %vpi_call 2 19 "$dumpvars", 32'sb00000000000000000000000000000000, S_0x5c259a72bcd0 {0 0 0};
    %pushi/vec4 0, 0, 32;
    %store/vec4 v0x5c259a7547a0_0, 0, 32;
T_2.0 ;
