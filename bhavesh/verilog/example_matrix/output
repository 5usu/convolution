    .scope S_0x5c259a73f6c0;
T_0 ;
    %wait E_0x5c259a704d80;
    %pushi/vec4 1, 0, 1;
    %store/vec4 v0x5c259a754300_0, 0, 1;
    %load/vec4 v0x5c259a754300_0;
    %pad/u 32;
    %cmpi/e 1, 0, 32;
    %jmp/0xz  T_0.0, 4;
    %pushi/vec4 0, 0, 32;
