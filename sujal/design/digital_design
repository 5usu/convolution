    %pushi/vec4 1, 0, 2;
    %cmp/u;
    %jmp/1 T_0.1, 6;
    %dup/vec4;
    %pushi/vec4 2, 0, 2;
    %cmp/u;
    %jmp/1 T_0.2, 6;
    %pushi/vec4 1431194446, 0, 32; draw_string_vec4
    %pushi/vec4 32, 0, 8; draw_string_vec4
    %concat/vec4; draw_string_vec4
    %ret/vec4 0, 0, 40;  Assign to state_to_string (store_vec4_to_lval)
    %jmp T_0.4;
T_0.0 ;
    %pushi/vec4 1092624416, 0, 32; draw_string_vec4
    %pushi/vec4 32, 0, 8; draw_string_vec4
    %concat/vec4; draw_string_vec4
    %ret/vec4 0, 0, 40;  Assign to state_to_string (store_vec4_to_lval)
    %jmp T_0.4;
T_0.1 ;
    %pushi/vec4 1109401632, 0, 32; draw_string_vec4
    %pushi/vec4 32, 0, 8; draw_string_vec4
    %concat/vec4; draw_string_vec4
    %ret/vec4 0, 0, 40;  Assign to state_to_string (store_vec4_to_lval)
    %jmp T_0.4;
T_0.2 ;
    %pushi/vec4 1126178848, 0, 32; draw_string_vec4
    %pushi/vec4 32, 0, 8; draw_string_vec4
    %concat/vec4; draw_string_vec4
